{
  "routes": [
    {
      "src": "/(.*).html",
      "headers": { "Content-Type": "text/html; charset=utf-8" },
      "dest": "/$1.html"
    },
    {
      "src": "/(.*).css",
      "headers": { "Content-Type": "text/css" },
      "dest": "/$1.css"
    },
    {
      "src": "/(.*).js",
      "headers": { "Content-Type": "application/javascript" },
      "dest": "/$1.js"
    },
    {
      "src": "/",
      "headers": { "Content-Type": "text/html; charset=utf-8" },
      "dest": "/index.html"
    }
  ]
}
